<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.restauranteapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RestauranteApp">
        <activity android:name=".ChatGrupalActivity"
            android:label="Chat grupal"></activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".principal"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".categorias"
            android:label="@string/title_activity_categorias"></activity>
        <activity
            android:name=".bebidas"
            android:label="@string/title_activity_bebidas"></activity>
        <activity
            android:name=".porciones"
            android:label="@string/title_activity_porciones"></activity>
        <activity
            android:name=".parrillada"
            android:label="@string/title_activity_parrillada"></activity>
        <activity
            android:name=".mariscos"
            android:label="@string/title_activity_mariscos"></activity>
        <activity
            android:name=".ControlMenu"
            android:label="@string/title_activity_control_menu"></activity>
        <activity
            android:name=".administrador"
            android:label="@string/title_activity_administrador"></activity>
        <activity
            android:name=".Carrito"
            android:label="@string/title_activity_carrilto"></activity>
        <activity
            android:name=".RegistroSms"
            android:label="@string/title_activity_registro_sms"></activity>
        <activity
            android:name=".Notas"
            android:label="@string/title_activity_notas"></activity>
        <activity
            android:name=".Pedido"
            android:label="@string/title_activity_pedido"></activity>

        <service
            android:name=".fcm"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>